.custom .title{
  text-align: center;
  font-size: 35rpx;
  font-weight: 600;
  color: rgba(255,255,255,1);
  height: 88rpx;
  line-height: 88rpx;
}
.custom .top {
  width: 100%;
  position:absolute;
  left: 0;
  z-index: -1;
}
/*.container{
  display: flex;
  flex-direction: column;
