.body{
  position: relative;
  display: flex;
  flex-direction: column;
}
.content{
  width:100%;
  height:189rpx;
  background:rgba(245,245,245,1);
  position: relative;
}
/*搜索框*/
.content .search-bar-box {
  width: 640rpx;
  margin: 0 auto;
  height: 90rpx;
  border-radius: 45rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 8px 10px 0px rgba(204,204,204,0.22);
  display: flex;
  justify-content: space-between;
  align-items: center;
  position:absolute;
  align-items: flex-end;
  top: -44rpx;
  left:50%;
  transform: translateX(-50%);
  z-index: 999;
  padding: 27rpx 20rpx;
  box-sizing: border-box;
}
 .search-bar-box .clear {
  height: 36rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-end;
  width: 54rpx;
}
.search-bar-box image {
  width: 36rpx;
  height: 36rpx;
}
.search-bar-box input {
  height: 36rpx;
  color: #37383a;
  font-size: 26rpx;
  line-height: 36rpx;
  min-height: 36rpx;
  flex-grow: 1;
  padding-left:10rpx;
}
/*拍照识别*/
.other .box{
  margin-right: 160rpx;
  margin-left: 154rpx;
  margin-top: 79rpx;
  display: flex;
  justify-content: space-between; 
}
.box .item {
  text-align: center;
}
.box .item .txt{
  height:20rpx;
  font-size:22rpx;
  font-family:SourceHanSansSC-Regular;
  font-weight:400;
  line-height: 20rpx;
  color:rgba(153,153,153,1);
}
.box .item .ar{
  width: 38rpx;
  height: 41rpx;
}
.box .item .photo{
  width: 45rpx;
  height: 35rpx;
}
.box .item .share{
  width: 42rpx;
  height: 34rpx;
}
